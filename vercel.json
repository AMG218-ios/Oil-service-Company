{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "builds": [
        { "src": "api/index.php", "use": "@vercel/php" },
        { "src": "dist/**", "use": "@vercel/static" }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "/api/index.php" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
